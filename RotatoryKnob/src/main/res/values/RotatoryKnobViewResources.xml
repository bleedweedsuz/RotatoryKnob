<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RotatoryKnobView">
        <attr name="RotatoryKnob_ImageBack" format="reference" />
        <attr name="RotatoryKnob_ImageActive" format="reference" />
        <attr name="RotatoryKnob_ImageInactive" format="reference" />
        <attr name="knobRotorTapButtonRes_ImageActive" format="reference" />
        <attr name="knobRotorTapButtonRes_ImageInActive" format="reference" />
        <attr name="RotatoryKnob_RotorValue" format="integer" />
        <attr name="RotatoryKnob_TapEnable" format="boolean"/>
        <attr name="RotatoryKnob_MinRotorValue" format="integer" />
        <attr name="RotatoryKnob_MaxRotorValue" format="integer" />
        <attr name="RotatoryKnob_RotorStep" format="integer" />
        <attr name="RotatoryKnob_EventType" format="enum">
            <enum name="RotatoryKnob_EventType_OnDownMove" value="0"/>
            <enum name="RotatoryKnob_EventType_OnScroll" value="1"/>
        </attr>
        <attr name="RotatoryKnob_KnobTapSize" format="dimension"/>
    </declare-styleable>

    <integer name="RotatoryKnob_Value_Default">0</integer>
    <integer name="RotatoryKnob_Min_Value">0</integer>
    <integer name="RotatoryKnob_Max_Value">100</integer>
    <integer name="RotatoryKnob_Step">1</integer>
    <dimen name="RotatoryKnobTap_Size">10dp</dimen>
</resources>